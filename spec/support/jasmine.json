{
  "spec_dir": "Spec",
  "spec_files": [
    "../src/tests/models/customerSpec.ts",
    "../src/tests/models/productSpec.ts",
    "../src/tests/models/orderSpec.ts",
    "../src/tests/models/adminSpec.ts",
    "../src/tests/handlers/adminSpec.ts",
    "../src/tests/handlers/customerSpec.ts",
    "../src/tests/handlers/productSpec.ts",
